    div(itemscope='' itemtype='https://schema.org/Article' style="display: none" hidden)
      meta(itemprop='url' content=`https://www.josephojo.com/${page.name}`)
      span(itemprop='name' content=`${page.title} - josephojo.com`)
        | by 
        span(itemprop='author') Joseph Ojo
          div(itemscope='' itemtype='https://schema.org/Person')
            img(itemprop='image' src='https://www.josephojo.com/android-icon-192x192.png' alt='The logo for josephojo.com')
            span(itemprop="email") laoluojo
              span &#64;
              span gmail.com
            span(itemprop='description')
              | The porfolio of Joseph, it contains all amazing projects etc...
            span(itemtype='https://schema.org/EducationalOrganization' itemprop="alumniOf")
              | OnTech - Ontario Tech University
            a(href='https://www.linkedin.com/in/o-josephojo/' itemprop='url') linkedin 
    if true
      //- dev
      //- Navbar etc...
      +header(page)
      
      mixin sectionView(section)
        if typeof section == "object"
          +section(section)

      mixin layoutView(layout)
        if typeof layout == "object"
          - let sectionList = layout.values;
          - let section = layout.section;
          +layout(layout)
            +sectionView(section)

            if Array.isArray(sectionList)
              each sections in sectionList
                - let section = sections.section;
                  +sectionView(section)

      main.main
        - let layerLists = page.values;

        //- Page Layers
        if Array.isArray(layerLists)
          each layers in layerLists
            - let layer = layers.layer;  //- Display Custom Layer if it exists
            - let hero = layers.hero; //- Display Hero Layer if it exists

            if typeof hero == "object"
              +hero(hero, page.banner)

            if typeof layer == "object"
              - let layout = layer.layout;
              - let layoutList = layer.values;
              +layer(layer)
                +layoutView(layout)

                if Array.isArray(layoutList)
                  each layouts in layoutList
                    - let layout = layouts.layout;
                    +layoutView(layout)

      .layer-action-center.layer-action-center-show
        article
          .layout-flex.layout-padding-horz
            section.layout-spacer
            section.layer-primary-dark.layer-shadow.layer-round
              button.back-to-top.btn.btn-hover.layer-round.layout-padding-horz-smallest.layout-padding-vert-smallest.layer-primary-dark.layer-primary-hover.layer-primary-focus.style-tertiary.style-hover-tertiary.style-focus-tertiary
                span.btn-group
                  i.icon.style-font-sz-24(inline) expand_less
                  span.btn-section
                    span.btn-spacer
                    span.btn-content Back To Top

                button.copyright-btn.btn.btn-hover.layer-round.layout-padding-horz-smallest.layout-padding-vert-smallest.layer-primary-dark.layer-primary-hover.layer-primary-focus.style-tertiary.style-hover-tertiary.style-focus-tertiary
                  span.btn-group.style-font-sz-24
                    i.icon(inline) copyright
                    span.btn-section
                      span.btn-spacer
                      span.btn-content 2019 Okiki Ojo

      if !page.noFooter
        +footer("Made with ‚ù§ by Okiki Ojo.")
    else
      section.layer.layer-dark
        article.layout-contain
          section.layout.layout-shorten.layer-overflow-hide.layout-max-height.layout-flex.layout-align-items-center.layout-justify-content-start
            section
              h3.style-secondary-dark.style-font-title.style-uppercase.layout-margin-dull-vert.layout-padding-vert-mini Joseph
              h1.style-white.style-bold.layer-title.style-uppercase.layout-margin-dull-vert-top Coming SOON
              h5.style-white.style-line-height.style-capital.layer-content-detail This website is currently in development. We are working day and night to have it in tip top shape.

